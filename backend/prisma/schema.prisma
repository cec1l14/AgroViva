generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produtor {
  cod_produtor Int       @id @default(autoincrement())
  email        String @unique
  telefone     String
  nome         String
  senha        String
  cpf          String
  produtos     Produto[] @relation("ProdutorProdutos")
}

model Produto {
  cod_produto  Int      @id @default(autoincrement())
  validade     String
  preco        Float
  descricao    String
  imagem       String
  tipo         String
  cod_produtor Int
  produtor     Produtor @relation("ProdutorProdutos", fields: [cod_produtor], references: [cod_produtor])
}




